  <section id="home" class="intro-module module-image heightfull bg-dark-alfa-30">

    <div class="intro">
      <div class="intro-slider">
        <!-- Slide 1 -->
        <div class="owl-item">
          <h1 class="intro-title">Best Coffee just a mile away</h1>
        </div>
        <!-- Slide 2 -->
        <div class="owl-item">
          <h1 class="intro-title">Make yourself delivered</h1>
        </div>
        <!-- Slide 3 -->
        <div class="owl-item">
          <h1 class="intro-title">Or Become the welcomed deliver</h1>
        </div>
      </div><!-- .intro-slider -->





      <div class="container"> <!-- home searchbar -->
        <div class="row">
          <div class="col-md-6 col-md-offset-3">
            <div id="custom-search-input">
              <form action="/shops" class="input-group col-md-12">
                <input type="text" name='address' class="form-control input-lg shop-search-input" placeholder="Search for a shop around you" />
                <span class="input-group-btn">
                <button  class="btn btn-info btn-lg" type="submit">
                  <i class="glyphicon glyphicon-search"></i>
                </button>
                </span>
              </form>
                </div>
            </div>
          </div><!-- end col -->
        </div><!-- end row -->
      </div><!-- end searchbar -->

    </div><!-- .intro -->

    <div class="mouse-icon">
      <div class="wheel"></div>
    </div>

  </section>

  <!-- Home end -->

  <!-- About start -->

  <section id="services" class="module">
    <div class="container">

      <div class="row">
